{
    "name": "alllights-game",
    "builds": [
      {
        "src": "/templates/index.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/templates/index.html"
        }
      ],
    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        }
      ]
  }