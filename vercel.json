{
    "name": "alllights-game",
    "builds": [
      {
        "src": "/templates/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "/static/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/templates/index.html"
        }
      ]
  }